                            <fo:block-container font-size="2em" line-height="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="0" page-break-after="avoid" id="ariaid-title67"><fo:block end-indent="0" start-indent="0" role="H1"><fo:marker marker-class-name="chaptertitle-no-prefix"><fo:inline/> 9 - <!--content(text)--><fo:inline>常见异常问题诊断</fo:inline></fo:marker><fo:marker marker-class-name="chaptertitle"><fo:inline/> | 9 - <!--content(text)--><fo:inline>常见异常问题诊断</fo:inline></fo:marker><fo:inline>Chapter 9. </fo:inline><fo:inline>常见异常问题诊断</fo:inline></fo:block></fo:block-container>
                            <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                <fo:block-container font-style="italic" space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"/></fo:block-container>
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="1em"><fo:block end-indent="0" start-indent="0"/></fo:block-container>
                            </fo:block></fo:block-container>
                            <fo:block role="Sect" id="unique_68" chfox:nd-id="_1068">
                                <fo:block-container font-size="1.7em" line-height="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid" id="ariaid-title68"><fo:block end-indent="0" start-indent="0" role="H2"><fo:inline>系统告警处理对策</fo:inline></fo:block></fo:block-container>
                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container font-style="italic" space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"/></fo:block-container>
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="1em"><fo:block end-indent="0" start-indent="0"/></fo:block-container>
                                </fo:block></fo:block-container>
                                <fo:block role="Sect" id="unique_69" chfox:nd-id="_1069">
                                    <fo:block-container font-size="1.4em" font-weight="300" line-height="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid" id="ariaid-title69"><fo:block end-indent="0" start-indent="0" role="H3"><fo:inline>制冷系统故障</fo:inline></fo:block></fo:block-container>
                                    <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                        <fo:block-container font-style="italic" space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"/></fo:block-container>
                                        <fo:block>
                                            <fo:table-and-caption border-collapse="collapse" space-before="1em" space-after="1em"><fo:table-caption start-indent="0" end-indent="0" keep-with-next="always"><fo:block-container font-weight="bold" space-before="0.5em" page-break-after="avoid" text-align="center"><fo:block end-indent="0" start-indent="0"/></fo:block-container></fo:table-caption><fo:table relative-align="baseline" chfox:html-table-width-policy="yes" table-layout="auto" border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1px" border-left-color="black" border-left-style="solid" border-left-width="1px" margin-left="0" border-right-color="black" border-right-style="solid" border-right-width="1px" margin-right="0" border-top-color="black" border-top-style="solid" border-top-width="1px" hyphenate="true" id="unique_69_Connect_42_table_ehr_wfy_5cc" chfox:nd-id="table_ehr_wfy_5cc" space-before="0" space-after="0"><fo:table-column column-width="33.333332%"/><fo:table-column column-width="33.333332%"/><fo:table-column column-width="33.333332%"/><fo:table-header font-weight="bold" text-align="center" display-align="center">
